    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.1.0",
    "rollup": "^2.56.2",
    "rollup-plugin-banner2": "^1.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.5",
    "typescript": "^4.3.5"
